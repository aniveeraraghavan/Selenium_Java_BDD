package com.healthasyst.datatypes;

public class DatatypesDemo {

   public static void main(String[] args) {
	 System.out.println();
	   
	   int radius = 10;
		 float pi= 3.14f;
			 float  area =(pi * radius * radius);//we can use double also
			 System.out.println (area);
					 String name = "Anitha";
					 System.out.println(name.charAt(2));
	   
	   
}
}