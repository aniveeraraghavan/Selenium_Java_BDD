package com.ha.shoppingcart2;

public class Shirt extends Item {
	
	private char size;
	private char colorCode;
	
	public Shirt (String description, int quantity, double prize, char size, char colorCode)
	{
		super("Shirt");
		super(5);
		super(1250.50);
		
		size = 'M' ;
		colorCode = 'r';
		
		
	}
	
	public void display() {
		System.out.println();
	}
}
