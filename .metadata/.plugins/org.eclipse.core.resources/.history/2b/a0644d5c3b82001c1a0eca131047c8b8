package com.HA.volume;

public class Voume {
	
	public static double volumeOfSphere(double r) {
		
		
		double sphere;
		return sphere = 4/3* Math.PI * Math.pow(r, r);
		
	}
	
	//VOLUME OF CYCLINDER
	public static double volumeOfCyclinder(double ra) {
		
		double cyclinder;
		int h = 12;
		return cyclinder = Math.PI * Math.pow(ra,ra) * h;
		
	}

	//volume of cuboid
	public static int volumeOfCuboid (int w, int l, int h) {
		
		int cuboid;
		return cuboid = Math.multiplyExact(w ,l ,h);
			}
	//voulme of cube
	
	public  static void volumeOfCube (long r, long h) {
		
		double cube = (1/3) * Math.PI * Math.pow(r, r) * h;
		System.out.println("Volume of Cube" +cube);
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
